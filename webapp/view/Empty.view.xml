<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="org.fater.app.controller.Empty"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page showHeader="false">
				<content>
					<MessagePage
							title="{i18n>emptyTitle}"
							text="{i18n>emptyText}"
							icon="{sap-icon://document}"
							description=""
							showNavButton="{device>/system/phone}"
							navButtonPress="onNavBack">
					</MessagePage>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>